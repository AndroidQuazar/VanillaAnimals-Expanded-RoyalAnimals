    <graphicData>
      <color>(243,237,237)</color>
    </graphicData>
    <statBases>
      <MarketValue>7.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>6.5</hatcherDaystoHatch>
        <hatcherPawn>VAERoy_Peacock</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  
  <!-- Pheasant -->

  <ThingDef ParentName="EggFertBase">
    <defName>VAERoy_EggPheasantFertilized</defName>
